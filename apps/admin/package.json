{
  "name": "@gamezone/admin",
  "version": "1.2.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:vercel": "pnpm --filter @gamezone/db exec prisma generate && pnpm build",
    "db:push:dev": "pnpm --filter @gamezone/db exec -- prisma db push",
    "db:migrate:dev": "pnpm --filter @gamezone/db exec -- prisma migrate dev",
    "db:push:prod": "dotenv -e .env.production -- pnpm --filter @gamezone/db exec prisma db push",
    "db:migrate:prod": "dotenv -e .env.production -- pnpm --filter @gamezone/db exec prisma migrate deploy",
    "db:studio:prod": "dotenv -e .env.production -- pnpm --filter @gamezone/db exec prisma studio"
  },
  "dependencies": {
    "@gamezone/db": "workspace:*",
    "@gamezone/lib": "workspace:*",
    "@gamezone/auth": "workspace:*",
    "apexcharts": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "cloudinary": "^2.6.1",
    "date-fns": "^4.1.0",
    "flatpickr": "^4.6.13",
    "jsvectormap": "^1.6.0",
    "next-themes": "^0.4.4",
    "nextjs-toploader": "^3.7.15",
    "react-apexcharts": "^1.7.0",
    "react-hot-toast": "^2.5.2",
    "tailwind-merge": "^2.6.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "dotenv-cli": "^8.0.0",
    "prisma": "^6.7.0"
  }
}